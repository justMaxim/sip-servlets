<?xml version="1.0" encoding="UTF-8"?>

<sip-app>
	<!-- Archetype Created SIP Application -->
	<app-name>com.berinchik.sip.FlexibleCommunicationSipApplication</app-name>
	
	<servlet-selection>
		<main-servlet>
			FlexibleCommunicationServlet
		</main-servlet>
	</servlet-selection>

	<servlet>
		<servlet-name>FlexibleCommunicationServlet</servlet-name>
		<display-name>FlexibleCommunicationServlet</display-name>
		<description>Implements simple register and configurable communication diversion</description>
		<servlet-class>
            com.berinchik.sip.FlexibleCommunicationService
        </servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>	
</sip-app>
